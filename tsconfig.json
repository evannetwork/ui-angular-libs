{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es5",
    "baseUrl": "./src",
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "../build",
    "rootDir": "./src",
    "lib": [
      "es2015",
      "dom"
    ],
    "paths": {
      "dapp-browser": ["../node_modules/@evan.network/ui-dapp-browser/runtime/build/main"]
    },
    "skipLibCheck": true,
    "types": []
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "angular-libs.js",
    "flatModuleId": "angular-libs"
  },
  "files": [
    "./index.ts"
  ]
}
